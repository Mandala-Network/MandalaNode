{
  "schema": "mandala-agent",
  "schemaVersion": "1.0",
  "agent": {
    "type": "agidentity",
    "runtime": "node"
  },
  "env": {
    "AGID_MODEL": "claude-sonnet-4-5-20250929",
    "LOG_LEVEL": "info"
  },
  "resources": {
    "cpu": "500m",
    "memory": "512Mi"
  },
  "ports": [3000],
  "healthCheck": {
    "path": "/health",
    "port": 3000,
    "intervalSeconds": 30
  },
  "frontend": {
    "directory": "frontend"
  },
  "storage": {
    "enabled": false
  },
  "databases": {
    "mysql": false,
    "mongo": false,
    "redis": true
  },
  "deployments": [
    {
      "provider": "mandala",
      "projectID": "your-project-id-here",
      "network": "mainnet"
    }
  ]
}
